#ifndef _METALLIC_CCTYPE
#define _METALLIC_CCTYPE

#include <ctype.h>

namespace std {

using ::iscntrl(int);
using ::isprint(int);
using ::isblank(int);
using ::isspace(int);
using ::isgraph(int);

using ::isupper(int);
using ::islower(int);
using ::isalpha(int);

using ::isdigit(int);
using ::isxdigit(int);

using ::isalnum(int);
using ::ispunct(int);

}

#endif // <cctype>

